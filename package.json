{
  "name": "ng-starter",
  "version": "0.0.1",
  "description": "Starter for Angular + ES6 + Gulp + Webpack",
  "main": "index.js",
  "scripts": {
    "build": "gulp build:production",
    "gulp": "gulp",
    "deploy": "gulp deploy",
    "version": "gulp rev uglify",
    "watch": "gulp webpack:watch watch",
    "test": "xo && ava",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "prepublish": "npm run build",
    "postpublish": "git push origin master --follow-tags"
  },
  "dependencies": {
    "angular": "^1.4.4",
    "angular-animate": "^1.4.8",
    "angular-aria": "^1.4.8",
    "angular-component": "0.0.3",
    "angular-material": "^1.0.2",
    "angular-ui-router": "^0.2.15"
  },
  "devDependencies": {
    "ava": "^0.10.0",
    "babel-eslint": "^4.1.6",
    "eslint": "^1.10.3",
    "eslint-config-xo": "^0.9.2",
    "eslint-plugin-ava": "0.0.1",
    "eslint-plugin-babel": "^3.0.0",
    "fannypack": "^1.3.0",
    "gulp": "^3.9.0",
    "xo": "^0.12.1"
  }
}
